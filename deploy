rsync . root@failsafe.network:~/fs -rv --exclude '.git' --exclude 'node_modules'

ssh root@failsafe.network <<'ENDSSH'
cd ~/fs
killall Failsafe

rm -rf private/db* private/Failsafe-* data

mkdir data
mkdir private
./install

node fs --genesis=main

forever stopall
forever start fs.js -p8000

ENDSSH

