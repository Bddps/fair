rsync . root@failsafe.network:~/fs -rv --exclude '.git' --exclude 'node_modules'

ssh root@failsafe.network <<'ENDSSH'
cd ~/fs
killall Failsafe

rm -rf private/db* private/Failsafe-* data

mkdir data
mkdir private
./install

node fs --genesis=main > /dev/null;

forever stopall;

cd ..
rsync -rva --exclude=private  --exclude=.git fs/ 8001
rsync -rva --exclude=private --exclude=.git fs/ 8002

cd 8001; forever start fs.js -p8001 --username=8001 --pw=password
cd ../8002; forever start fs.js fs.js -p8002 --username=8002 --pw=password
cd ../fs; forever start fs.js fs.js -p8000

cat private/pk.json



ENDSSH

